<?php

/**
 * Having discretional configuration by environment, including sandboxes.
 *
 * @param $target_env
 * @param $current_env
 */
function labp_environment_environment_switch($target_env, $current_env) {
  if ($target_env != 'production') {
    variable_set('googleanalytics_account', 'UA-74074646-2');
    drupal_set_message('Set SOLR environment');
  }
}